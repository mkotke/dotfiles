set t_Co=256
colorscheme badwolf
syntax enable
set tabstop=4
set softtabstop=4
set expandtab
set relativenumber
set number
set showcmd
filetype indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch
let mapleader="," 
nnoremap <leader><space> :nohlsearch<CR>
set pastetoggle=<F10>
